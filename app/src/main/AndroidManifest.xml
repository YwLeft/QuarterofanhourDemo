<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asus.quarterofanhourdemo">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:name=".model.net.MyApp"
        android:roundIcon="@drawable/raw_1499933216"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.acyivity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.acyivity.TheHomePageActivity" />
        <activity android:name=".view.acyivity.SetUpThesActivity"/>
        <activity android:name=".view.acyivity.LoginOneActivity"/>
        <activity android:name=".view.acyivity.LoginTwoActivity"/>
        <activity android:name=".view.acyivity.CreationActivity"/>
        <activity android:name=".view.acyivity.ReferrerActivity"/>
    </application>

</manifest>